def square(num):
    return num**2

def factorial(num):
    product = 1
    for i in range(1,num+1):
        product = product*i
    return product
        
def is_even(num):
    if(num%2==0):
        return True
    return False

def main():
    num = int(input())
    print(square(num))
    print(factorial(num))
    print(is_even(num))

if __name__ == "__main__":
    main()
